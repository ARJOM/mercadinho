<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>


<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<AnchorPane prefHeight="443.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.budega.controller.ListarClienteController">
<children>
    <Label layoutX="212.0" layoutY="24.0" styleClass="titleFuncionario" stylesheets="@../style/Main.css" text="Clientes Cadastrados" />
    <TableView fx:id="tabelaCliente" layoutX="13.0" layoutY="80.0" prefHeight="339.0" prefWidth="600.0">
        <columns>
            <TableColumn fx:id="colunaCpf" prefWidth="150" text="CPF" />
            <TableColumn fx:id="colunaNome" prefWidth="150" text="Nome" />
            <TableColumn fx:id="colunaContato" prefWidth="150" text="Contato" />
            <TableColumn fx:id="colunaDivida" prefWidth="150" text="Divida" />
        </columns>
    </TableView>
    <TextField layoutX="629.0" layoutY="122.0" prefHeight="22.0" prefWidth="198.0" styleClass="input" />
    <Button layoutX="835.0" layoutY="122.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="44.0" styleClass="btnBuscar" />
    <Button fx:id="btnDeletarCliente" layoutX="630.0" layoutY="370.0" mnemonicParsing="false" onAction="#deleteCliente" prefHeight="38.0" prefWidth="109.0" styleClass="btnDeletar" text="Deletar" />
    <Button fx:id="btnAtualizarCliente" layoutX="760.0" layoutY="370.0" mnemonicParsing="false" onAction="#atualizarCliente" prefHeight="45.0" prefWidth="109.0" styleClass="btnCadastrar" text="Atualizar" />
</children>
</AnchorPane>
