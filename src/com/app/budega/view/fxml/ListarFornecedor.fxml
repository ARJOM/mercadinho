                <TableColumn fx:id="colunaCnpj" prefWidth="150.0" text="CNPJ" />
                <TableColumn fx:id="colunaNome" prefWidth="150.0" text="Nome" />
                <TableColumn fx:id="colunaContato" prefWidth="150.0" text="Contato" />
