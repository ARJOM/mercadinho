        </TableView>
        <TextField layoutX="479.0" layoutY="122.0" prefHeight="22.0" prefWidth="198.0" styleClass="input" />
        <Button layoutX="685.0" layoutY="122.0" mnemonicParsing="false" prefHeight="45.0" prefWidth="44.0" styleClass="btnBuscar" />
        <Button fx:id="btnDeletarProduto" layoutX="496.0" layoutY="384.0" mnemonicParsing="false" onAction="#deleteProduto" prefHeight="38.0" prefWidth="109.0" styleClass="btnDeletar" text="Deletar" />
        <Button fx:id="btnAtualizarProduto" layoutX="620.0" layoutY="384.0" mnemonicParsing="false" onAction="#atualizarProduto" prefHeight="45.0" prefWidth="109.0" styleClass="btnCadastrar" text="Atualizar" />
