<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.app.budega.controller.CadastroAbateDividaController">

    <children>
        <Pane prefHeight="471.0" prefWidth="314.0" styleClass="gradiente" stylesheets="@../style/Main.css">
            <children>
                <Pane layoutX="63.0" layoutY="143.0" prefHeight="186.0" prefWidth="188.0">
                    <styleClass>
                        <String fx:value="img" />
                        <String fx:value="imgCliente" />
