                <TextField fx:id="campoValor" layoutX="99.0" layoutY="242.0" prefHeight="42.0" prefWidth="279.0" styleClass="input" stylesheets="@../style/Main.css" />

                <Button layoutX="165.0" layoutY="372.0" mnemonicParsing="false" onAction="#acaoAbater" prefHeight="48.0" prefWidth="146.0" styleClass="btnCadastrar" stylesheets="@../style/Main.css" text="Abater Dívida" />
                <Label layoutX="210.0" layoutY="437.0" styleClass="btnCancelar" text="Cancelar" />
